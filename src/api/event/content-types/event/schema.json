{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "start_time": {
      "type": "datetime",
      "required": true
    },
    "end_time": {
      "type": "datetime",
      "required": true
    },
    "event_type": {
      "type": "string"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "location": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::location.location",
      "inversedBy": "event_location"
    },
    "tickets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ticket.ticket"
    },
    "ticket_types": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ticket-type.ticket-type"
    },
    "ticket": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ticket.ticket",
      "inversedBy": "event"
    },
    "ticket_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ticket-type.ticket-type",
      "inversedBy": "event"
    },
    "organizer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::organizer.organizer",
      "inversedBy": "event"
    }
  }
}
